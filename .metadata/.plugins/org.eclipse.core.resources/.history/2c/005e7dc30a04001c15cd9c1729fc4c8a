package review;

import java.util.Scanner;

public class WT03 {
	
	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		
		System.out.println("a>");
		int a = scan.nextInt();
		System.out.println("b>");
		int b = scan.nextInt();
		
		if( a > b ) {
			System.out.println(">");
		} else if (a < b) {
			System.out.println("<");
		} else if (a == b) {
			System.out.println("==");
		}
	}
}
