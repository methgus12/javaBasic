package review;

import java.util.Scanner;

public class WorkTest20 {
	
	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		int a,b;
		System.out.println("a입력>");
		a= scan.nextInt();
		System.out.println("b입력>");
		b= scan.nextInt();
		
		
		System.out.println(a+b);
	}
	
	

}
